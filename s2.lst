0001BFEC                            ; ---------------------------------------------------------------------------
0001BFEC                            ; "MAIN LEVEL LOAD BLOCK" (after Nemesis)
0001BFEC                            ;
0001BFEC                            ; This struct array tells the engine where to find all the art associated with
0001BFEC                            ; a particular zone. Each zone gets four longwords, in which it stores four
0001BFEC                            ; pointers (in the lower 24 bits) and three jump table indeces (in the upper eight
0001BFEC                            ; bits). The assembled data looks something like this:
0001BFEC                            ;
0001BFEC                            ; aaBBBBBB
0001BFEC                            ; ccDDDDDD
0001BFEC                            ; EEEEEE
0001BFEC                            ; ffgghhii
0001BFEC                            ;
0001BFEC                            ; aa = index for primary pattern load request list
0001BFEC                            ; BBBBBB = unused, pointer to level art
0001BFEC                            ; cc = index for secondary pattern load request list
0001BFEC                            ; DDDDDD = pointer to 16x16 block mappings
0001BFEC                            ; EEEEEE = pointer to 128x128 block mappings
0001BFEC                            ; ff = unused, always 0
0001BFEC                            ; gg = unused, music track
0001BFEC                            ; hh = unused, palette
0001BFEC                            ; ii = palette
0001BFEC                            ;
0001BFEC                            ; Nemesis refers to this as the "main level load block". However, that name implies
0001BFEC                            ; that this is code (obviously, it isn't), or at least that it points to the level's
0001BFEC                            ; collision, object and ring placement arrays (it only points to palettes and 16x16
0001BFEC                            ; mappings although the 128x128 mappings do affect the actual level layout and collision)
0001BFEC                            ; ---------------------------------------------------------------------------
0001BFEC                            
0001BFEC                            ; macro for declaring a "main level load block" (MLLB)
0001BFEC                            levartptrs macro plc1,plc2,palette,art,map16x16,map128x128,music
0001BFEC                            	dc.l art+(plc1<<24)
0001BFEC                            	dc.l map16x16+(plc2<<24)
0001BFEC                            	dc.l map128x128
0001BFEC                            	dc.b 0,music,palette,palette
0001BFEC                            	endm
